grid'\nimport GridGuide from './grid-guide'\nimport Area from './area'\nimport FullscreenDemo from '../../_demo/fullscreen-demo'\n\nconst makeShortcode = name => function MDXDefaultShortcode(props) {\n  console.warn(\"Component \" + name + \" was not imported, exported, or provided by MDXProvider as global scope\")\n  return <div {...props}/>\n};\n\nconst layoutProps = {\n  \n};\nconst MDXLayout = \"wrapper\"\nexport default function MDXContent({\n  components,\n  ...props\n}) {\n  return <MDXLayout {...layoutProps} {...props} components={components} mdxType=\"MDXLayout\">\n\n\n    <h1 {...{\n      \"id\": \"grid\"\n    }}>{`Grid`}</h1>\n    <p>{`Grids do not have a gap or multiple rows and arrange their areas according to specific templates.`}</p>\n    <FullscreenDemo variant=\"dark\" of={Grid} title=\"Grid\" mdxType=\"FullscreenDemo\">\n  <h1>Halves</h1>\n  <h2>Fluid</h2>\n  <Grid colorway=\"primary\" mdxType=\"Grid\">\n    <Area colorway=\"secondary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"tertiary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <Grid colorway=\"primary\" mdxType=\"Grid\">\n    <Area colorway=\"tertiary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"secondary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <h2>Contained</h2>\n  <GridGuide mdxType=\"GridGuide\" />\n  <Grid colorway=\"primary\" contained mdxType=\"Grid\">\n    <Area colorway=\"secondary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"tertiary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <Grid colorway=\"primary\" contained mdxType=\"Grid\">\n    <Area colorway=\"tertiary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"secondary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <h1>Thirds</h1>\n  <h2>Fluid</h2>\n  <Grid colorway=\"primary\" variant=\"thirds\" mdxType=\"Grid\">\n    <Area colorway=\"secondary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"tertiary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <Grid colorway=\"primary\" variant=\"thirds\" mdxType=\"Grid\">\n    <Area colorway=\"tertiary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"secondary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <h2>Contained</h2>\n  <GridGuide mdxType=\"GridGuide\" />\n  <Grid colorway=\"primary\" variant=\"thirds\" contained mdxType=\"Grid\">\n    <Area colorway=\"secondary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"tertiary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n  <Grid colorway=\"primary\" variant=\"thirds\" contained mdxType=\"Grid\">\n    <Area colorway=\"tertiary\" area=\"left\" mdxType=\"Area\">Left</Area>\n    <Area colorway=\"secondary\" area=\"right\" mdxType=\"Area\">Right</Area>\n  </Grid>\n    </FullscreenDemo>\n    <h2 {...{\n      \"id\": \"props\"\n    }}>{`Props`}</h2>\n    <Props of={Grid} mdxType=\"Props\" />\n    </MDXLayout>;\n}\n\n;\nMDXContent.isMDXComponent = true;\n  "]}]}